{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"docs#build": {
			"dependsOn": ["ui#build"],
			"outputs": ["storybook-static/**"]
		},
		"web#build": {
			"dependsOn": ["ui#build"],
			"outputs": [".next/**"]
		},
		"ui#build": {
			"inputs": ["src/**"]
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
